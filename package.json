{
  "name": "know-your-macros",
  "version": "0.0.0",
  "description": "An application which allows users to calculate their ideal meal to reach a desired macronutrient quantity goal",
  "repository": {
    "type": "git",
    "url": "git://github.com/lucas1richard/knowyourmacros.git"
  },
  "engines": {
    "npm": ">=5",
    "node": "9.11.2"
  },
  "author": "Richard Lucas",
  "license": "MIT",
  "scripts": {
    "db:sync": "node ./server/db/sync",
    "db:sync:force": "FORCE_DB_SYNC=true node ./server/db/sync",
    "db:seed": "node ./server/db/seed",
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "heroku-postbuild": "npm run heroku-postbuild-seed",
    "heroku-postbuild-seed": "node ./server/db/seed/seedFilteredMeals",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "start:dev": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start": "npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test:server": "NODE_ENV=test nyc mocha --timeout=4000 --recursive --exit ./server/**/*.test.js && npm run nyc-coverage",
    "nyc-coverage": "nyc report --reporter=html",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "babel": {
    "plugins": [
      "styled-components"
    ],
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "stage-0"
    ]
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "@kym/db": "file:kym-db-0.0.0.tgz",
    "axios": "^0.18.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest": "^6",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chalk": "2.4.1",
    "compression": "1.7.2",
    "cors": "^2.8.4",
    "cross-env": "5.2.0",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "crypto-md5": "^1.0.0",
    "delay": "^3.0.0",
    "dotenv": "^6.1.0",
    "express": "4.16.3",
    "glob": "^7.1.3",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "joi": "^13.6.0",
    "jwt-simple": "^0.5.1",
    "lodash": "4.17.10",
    "minimist": "1.2.0",
    "mocha": "^5.2.0",
    "module": "^1.2.5",
    "moment": "^2.22.2",
    "net": "^1.0.2",
    "newrelic": "^4.9.0",
    "ngrok": "3.0.1",
    "offline-plugin": "5.0.5",
    "passport": "^0.4.0",
    "passport-fitbit-oauth2": "^1.0.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^7.5.0",
    "pg-hstore": "^2.3.2",
    "pg-native": "^3.0.0",
    "pluralize": "^7.0.0",
    "prop-types": "15.6.1",
    "query-string": "^6.1.0",
    "raven": "^2.6.4",
    "redis": "^2.8.0",
    "sequelize": "^4.39.1",
    "sinon": "^7.0.0",
    "sparkpost": "^2.1.2",
    "stripe": "^6.12.1",
    "supertest": "^3.3.0",
    "swagger-ui-express": "^4.0.1",
    "swipe-js-iso": "^2.0.4",
    "throng": "^4.0.0",
    "tls": "^0.0.1",
    "warning": "4.0.1",
    "whatwg-fetch": "2.0.4",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/connect-mongo": "^0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/jest": "^22.1.3",
    "@types/jquery": "^3.2.17",
    "@types/lodash": "^4.14.91",
    "@types/lusca": "^1.5.0",
    "@types/mongodb": "^3.0.5",
    "@types/mongoose": "^4.7.34",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.6",
    "@types/nodemailer": "^4.3.4",
    "@types/passport": "^0.4.3",
    "@types/passport-facebook": "^2.1.7",
    "@types/passport-local": "^1.0.32",
    "@types/request": "^2.47.0",
    "@types/shelljs": "^0.7.8",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.7",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "coveralls": "3.0.1",
    "enzyme": "3.3.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "html-loader": "0.5.5",
    "husky": "0.14.3",
    "imports-loader": "0.8.0",
    "lint-staged": "7.2.0",
    "node-plop": "0.15.0",
    "null-loader": "0.1.1",
    "plop": "2.0.0",
    "pre-commit": "1.2.2",
    "prettier": "1.13.5",
    "rimraf": "2.6.2",
    "shelljs": "0.8.2"
  }
}
